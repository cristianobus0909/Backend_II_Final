<h1>Carrito</h1>
<ul>
    {{#each cart.products}}
    <li>
        <h2>{{this.product.title}}</h2>
        <p>Precio: ${{this.product.price}}</p>
        <p>Cantidad: {{this.quantity}}</p>
    </li>
    {{/each}}
</ul>
<a href="/products">Volver a la lista de productos</a>



<script src="/js/cart.js"></script>